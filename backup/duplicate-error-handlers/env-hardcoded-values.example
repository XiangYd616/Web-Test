# 硬编码值配置化后的环境变量示例

# 服务器配置
BACKEND_URL=http://localhost:3001
FRONTEND_URL=http://localhost:5174
BACKEND_HOST=localhost
BACKEND_PORT=3001
FRONTEND_HOST=localhost
FRONTEND_PORT=5174

# API配置
REQUEST_TIMEOUT=30000
MAX_CONCURRENCY=10
RETRY_ATTEMPTS=3
MAX_FILE_SIZE=50mb

# 数据库配置
DB_PORT=5432

# 第三方服务
GITHUB_API_URL=https://api.github.com

# 说明：这些环境变量用于替代代码中的硬编码值
# 在生产环境中，请根据实际需求调整这些值
