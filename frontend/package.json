{
  "name": "testweb-frontend",
  "version": "1.0.0",
  "description": "Test Web App Frontend",
  "type": "module",
  "private": true,
  "scripts": {
    "_comment_dev": "🚀 开发相关",
    "dev": "cross-env VITE_DEV_PORT=5174 vite --host --config ../config/build/vite.config.ts",
    "build": "cross-env NODE_ENV=production vite build --config ../config/build/vite.config.ts",
    "build:check": "tsc --noEmit && vite build --config ../config/build/vite.config.ts",
    "preview": "vite preview --config ../config/build/vite.config.ts",
    "_comment_test": "🧪 测试相关",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "_comment_maintenance": "🔧 代码维护",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,css,md}\""
  },
  "dependencies": {
    "axios": "^1.11.0",
    "chart.js": "^4.4.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^6.20.1",
    "recharts": "^2.15.3",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.31",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.8.3",
    "vite": "^4.5.0",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
