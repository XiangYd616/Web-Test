{
  "name": "test-web-frontend",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "description": "Test Web App Frontend - React Application",
  "main": "main.tsx",
  "scripts": {
    "dev": "vite --host",
    "dev-safe": "cross-env NODE_ENV=development vite --host --port 5174",
    "build": "vite build",
    "build-safe": "cross-env NODE_ENV=production tsc --noEmit && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "type-ignore": "tsc --noEmit --skipLibCheck",
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "axios": "^1.11.0",
    "antd": "^5.27.1",
    "@ant-design/icons": "^6.0.0",
    "@mui/material": "^7.3.2",
    "@mui/icons-material": "^7.3.2",
    "@mui/lab": "^7.0.0-beta.17",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@heroicons/react": "^2.2.0",
    "lucide-react": "^0.544.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.3.1",
    "chart.js": "^4.5.0",
    "react-chartjs-2": "^5.3.0",
    "recharts": "^2.15.3",
    "date-fns": "^4.1.0",
    "react-datepicker": "^8.7.0",
    "react-hot-toast": "^2.6.0",
    "ahooks": "^3.9.4",
    "jwt-decode": "^4.0.0",
    "qrcode.react": "^4.2.0",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/react-datepicker": "^7.0.0",
    "@vitejs/plugin-react": "^4.1.1",
    "typescript": "^5.9.2",
    "vite": "^4.5.0",
    "vitest": "^1.6.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/user-event": "^14.6.1",
    "jsdom": "^26.1.0",
    "cross-env": "^7.0.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
