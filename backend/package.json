{
  "name": "testweb-api-server",
  "version": "1.0.0",
  "description": "Test Web App API Server",
  "main": "src/app.js",
  "scripts": {
    "_comment_main": "🚀 主要启动命令",
    "setup": "node scripts/setup-wizard.js",
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "dev:debug": "node --inspect src/app.js",
    "_comment_test": "🧪 测试相关",
    "test": "jest --config jest.config.js",
    "test:ui": "jest --config jest.config.js --watch",
    "test:run": "jest --config jest.config.js --run",
    "test:watch": "jest --config jest.config.js --watch",
    "test:coverage": "jest --config jest.config.js --coverage",
    "_comment_maintenance": "🔧 代码维护",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "_comment_database": "🗄️ 数据库操作",
    "db:init": "node scripts/initDatabase.js init",
    "db:reset": "node scripts/initDatabase.js reset",
    "db:clean": "node scripts/initDatabase.js clean",
    "db:status": "node scripts/initDatabase.js status",
    "db:seed": "node scripts/seedDatabase.js seed",
    "db:seed:clean": "node scripts/seedDatabase.js clean",
    "db:migrate": "node scripts/migrateDatabase.js migrate",
    "db:migrate:status": "node scripts/migrateDatabase.js status",
    "db:migrate:validate": "node scripts/migrateDatabase.js validate",
    "db:migrate:create": "node scripts/migrateDatabase.js create",
    "db:validate": "node scripts/validateDatabaseStructure.js",
    "db:backup": "node scripts/migrate.js backup",
    "_comment_docs": "📚 文档相关",
    "docs:generate": "node scripts/generateApiDocs.js generate",
    "docs:validate": "node scripts/generateApiDocs.js validate",
    "docs:serve": "npx http-server docs -p 8080 -o",
    "_comment_logs": "📋 日志管理",
    "logs:analyze": "node scripts/logManager.js analyze",
    "logs:cleanup": "node scripts/logManager.js cleanup",
    "logs:archive": "node scripts/logManager.js archive",
    "logs:monitor": "node scripts/logManager.js monitor",
    "logs:stats": "node scripts/logManager.js stats",
    "_comment_performance": "🚀 性能优化",
    "perf:analyze": "node scripts/performanceOptimizer.js analyze",
    "perf:cleanup": "node scripts/performanceOptimizer.js cleanup",
    "perf:reset": "node scripts/performanceOptimizer.js reset",
    "_comment_security": "🔒 安全相关",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "_comment_health": "🏥 健康检查",
    "health:check": "curl http://localhost:3001/health",
    "_comment_cache": "💾 缓存管理",
    "cache:stats": "curl http://localhost:3001/api/test/cache/stats",
    "cache:flush": "curl -X POST http://localhost:3001/api/test/cache/flush",
    "_comment_redis": "🔴 Redis操作",
    "redis:check": "node ../scripts/backend/check-redis.js",
    "_comment_docker": "🐳 Docker操作",
    "docker:build": "docker build -f ../deploy/Dockerfile.api -t testweb-api .",
    "docker:run": "docker run -p 3001:3001 testweb-api",
    "build": "tsc"
  },
  "dependencies": {
    "@isaacs/fs-minipass": "^4.0.1",
    "@socket.io/redis-adapter": "^8.3.0",
    "abort-controller": "^3.0.0",
    "archiver": "^7.0.1",
    "axios": "^1.11.0",
    "bcrypt": "^5.1.1",
    "bull": "^4.12.2",
    "cheerio": "^1.1.0",
    "chrome-launcher": "^1.2.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "csv-parser": "^3.2.0",
    "csv-writer": "^1.6.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.21.2",
    "express-openapi-validator": "^5.3.7",
    "express-rate-limit": "^7.4.1",
    "express-validator": "^7.2.0",
    "fast-csv": "^5.0.2",
    "helmet": "^8.0.0",
    "http-proxy-agent": "^7.0.2",
    "https-proxy-agent": "^7.0.5",
    "ioredis": "^5.4.1",
    "joi": "^17.13.3",
    "lighthouse": "^12.8.2",
    "lodash": "^4.17.21",
    "maxmind": "^4.3.8",
    "mongodb": "^6.17.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "mysql2": "^3.14.1",
    "node-cron": "^3.0.3",
    "node-fetch": "^2.7.0",
    "nodemailer": "^6.10.1",
    "pdf-lib": "^1.17.1",
    "pdfkit": "^0.17.1",
    "pg": "^8.16.2",
    "pg-hstore": "^2.3.4",
    "playwright": "^1.53.1",
    "prom-client": "^13.2.0",
    "puppeteer": "^24.10.2",
    "qrcode": "^1.5.4",
    "redis": "^5.5.6",
    "sequelize": "^6.37.5",
    "sharp": "^0.33.5",
    "socket.io": "^4.8.1",
    "speakeasy": "^2.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tar": "^7.4.3",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.18.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.0",
    "bcryptjs": "^3.0.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.15.0",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2",
    "supertest": "^7.1.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "test",
    "web",
    "api",
    "performance",
    "security",
    "seo",
    "monitoring",
    "postgresql",
    "express",
    "javascript"
  ],
  "author": "Test Web App Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https:/github.com/your-org/test-web-app.git"
  },
  "bugs": {
    "url": "https:/github.com/your-org/test-web-app/issues"
  },
  "homepage": "https:/github.com/your-org/test-web-app#readme",
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/dist/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 75,
        "lines": 75,
        "statements": 75
      }
    }
  },
  "eslintConfig": {
    "env": {
      "node": true,
      "jest": true,
      "es2021": true
    },
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "no-console": "warn",
      "no-unused-vars": "error"
    }
  },
  "nodemonConfig": {
    "watch": [
      "."
    ],
    "ext": "js,json",
    "ignore": [
      "node_modules/",
      "dist/",
      "coverage/",
      "logs/"
    ]
  }
}
